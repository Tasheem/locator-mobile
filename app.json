{
  "expo": {
    "name": "Locator",
    "slug": "Locator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "app/assets/locator_center_solid.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "app/assets/locator_center_solid.png",
      "resizeMode": "contain",
      "backgroundColor": "#292524"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.locator.locator"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "app/assets/locator_center_solid.png",
        "backgroundColor": "#292524"
      },
      "supportsTablet": false,
      "package": "com.locator.locator"
    },
    "web": {
      "favicon": "app/assets/locator_center_solid.png"
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
